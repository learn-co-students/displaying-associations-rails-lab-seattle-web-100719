<h1>Artists</h1>
<% @artists.each do |artist| %>
    <h2><%= link_to artist.name, artist_path(artist) %></h2>
    <h3><%= pluralize(artist.songs.count, "song") %></h3>
    <ul>
    <% artist.songs.each do |song| %>
        <li><%= link_to song.title, song_path(song) %></li>
    <% end %>
    </ul>
<% end %>